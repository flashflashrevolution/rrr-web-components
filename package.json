{
  "name": "rrr-web-components",
  "version": "1.0.0",
  "private": true,
  "repository": "git@github.com:flashflashrevolution/rrr-web-components.git",
  "license": "MIT",
  "type": "module",
  "author": "Zageron <hello@zageron.ca>",
  "scripts": {
    "build": "run-s build:postcss build:vite",
    "build:postcss": "postcss src --base src --dir dist --verbose",
    "build:vite": "vite build",
    "dev": "run-p \"dev:*\"",
    "dev:postcss": "postcss src --base src --dir dist --verbose --watch",
    "dev:vite": "vite",
    "lint": "run-p --print-label \"lint:*\"",
    "lint:eslint": "eslint --ignore-path .gitignore -f pretty --color \"**/*.{cjs,ts}\"",
    "preview": "vite preview"
  },
  "dependencies": {
    "lit": "^2.2.8"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.5",
    "@types/node": "^18.6.2",
    "@types/prettier": "^2.6.4",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "postcss-lit": "^0.4.3",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "typescript": "^4.7.4",
    "vite": "^3.0.3",
    "vite-plugin-checker": "^0.4.9"
  },
  "packageManager": "yarn@4.0.0-rc.13"
}
